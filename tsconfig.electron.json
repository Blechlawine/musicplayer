{
    "compilerOptions": {
        "target": "es6",
        "useDefineForClassFields": true,
        "module": "CommonJs",
        "moduleResolution": "node",
        "strict": true,
        "sourceMap": false,
        "esModuleInterop": true,
        "outDir": "./electronCompiled",
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
    },
    "include": ["electron/**/*.ts"]
}
